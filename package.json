{
  "name": "kubiquity",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "rm -rf build && webpack --mode production --config webpack.common.js",
    "dev": "rm -rf build && concurrently 'webpack --config webpack.common.js --watch --mode development' 'electron .'",
    "frontend": "rm -rf build && concurrently 'webpack --config webpack.common.js --watch  --mode development' 'electron .'",
    "backend": "nodemon server/server.js",
    "react": "rm -rf build && webpack --config webpack.common.js --watch  --mode development",
    "test": "jest --verbose",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=src/assets/images/icon.png --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/images/icon.ico --prune=true --out=release-builds --version-string.CompanyName=K8sM8s --version-string.FileDescription=CE --version-string.ProductName=\"Kubiquity\"",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/Kubiquity.git"
  },
  "author": "k8sm8s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/Kubiquity/issues"
  },
  "homepage": "https://github.com/oslabs-beta/Kubiquity#readme",
  "dependencies": {
    "@kubernetes/client-node": "^0.15.0",
    "apexcharts": "^3.27.1",
    "child_process": "^1.0.2",
    "dotenv": "^10.0.0",
    "electron": "^13.1.4",
    "electron-json-storage": "^4.5.0",
    "electron-reload": "^1.5.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.1",
    "node-cmd": "^4.0.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-csv-downloader": "^2.7.0",
    "react-dom": "^17.0.2",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "electron-packager": "^15.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
